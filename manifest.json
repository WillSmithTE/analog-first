{
  "name": "Analog First",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Add a box breathing widget to any website",
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {}
}
