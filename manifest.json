{
  "name": "Breathe In",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Improve your downtime with a portable breathing widget on every website",
  "permissions": ["storage", "activeTab"],
  "host_permissions": [],
  "optional_host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {}
}
